@page
@model HilleroedSejlklubExam.Pages.Members.DeleteMemberModel
@{
    ViewData["Title"] = "Delete Customer";
}
<h1>Delete Customer</h1>

<form method="post">
    <input type="hidden" asp-for="Member.Id" />
    <div class="form-group">
        <label asp-for="Member.Name" class="control-label">Name</label>
        <input asp-for="Member.Name" class="form-control" disabled />
    </div>
    <div class="form-group">
        <label asp-for="Member.Email" class="control-label">Email</label>
        <input asp-for="Member.Email" class="form-control" disabled />
    </div>
    <div class="form-group">
        <label asp-for="Member.Birthday" class="control-label">Birthday</label>
        <input asp-for="Member.Birthday" type="date" class="form-control" disabled />
    </div>
    <div class="form-group">
        <label asp-for="Member.Phone" class="control-label">Phone</label>
        <input asp-for="Member.Phone" class="form-control" disabled />
    </div>

    <div class="formgroup">
        <input type="submit" value="Slet" class="btn btn-danger" />
    </div>
</form>

<div>
    <a asp-page="/Members/ShowMembers">Tilbage til medlemmer</a>
</div>
