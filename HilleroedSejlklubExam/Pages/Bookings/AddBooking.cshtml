@page
@model HilleroedSejlklubExam.Pages.Bookings.AddBookingModel
@{
    ViewData["Title"] = "Add Booking";
}

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <div asp-validation-summary="All" class="text-danger"></div>
        <label asp-for="@Model.Booking.Id" class="control-label"></label>
        <input asp-for="@Model.Booking.Id" class="form-control" />
        <span asp-validation-for="Booking.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Booking.Date" class="control-label"></label>
        <input asp-for="@Model.Booking.Date" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Booking.Duration" class="control-label"></label>
        <input asp-for="@Model.Booking.Duration" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Booking.End" class="control-label"></label>
        <input asp-for="@Model.Booking.End" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Booking.BookedBy" class="control-label"></label>
        <input asp-for="@Model.Booking.BookedBy" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Booking.BookedDinghy" class="control-label"></label>
        <input asp-for="@Model.Booking.BookedDinghy" class="form-control" />
    </div>

    <div class="formgroup">
        <input type="submit" value="Lav Booking" class="btn btn-primary" />
    </div>
</form>

